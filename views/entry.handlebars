<body>
	<div class="container">
		<!-- h1 through h6 are headers. The higher the number, the smaller the header -->
		<!-- p means paragraph -->
		<div class="jumbotron">
			<h1>Entry</h1>
		</div>


		<a href="/">
			<img src="images/new_challenge.jpg" alt="nvm, back to main menu">
		</a>

		<form name="log" role="form"  method="get" action="/add" >
			<h3>Write a log!</h3>
			<div class="form-group">
				<label for="name">Title:</label>
				<input type="text" class="form-control" id="name" placeholder="Title" name="title">
			</div>
			<div class="form-group">
				<label for="description">Log:</label>
				<input type="text" class="form-control" id="description" placeholder="Log" name="log">
			</div>
		  <input type="submit" id="submitBtn" class="btn btn-default" value="Submit the Log"></input>
		</form>



	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
</body>
